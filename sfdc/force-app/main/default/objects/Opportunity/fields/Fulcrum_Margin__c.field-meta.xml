<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fulcrum_Margin__c</fullName>
    <description>Calculated gross profit margin (Revenue * GP%)</description>
    <formula
  >IF(ISBLANK(Fulcrum_Revenue__c) || ISBLANK(Fulcrum_GP_Percent__c), NULL, Fulcrum_Revenue__c * Fulcrum_GP_Percent__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Margin</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
